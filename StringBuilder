package Strings;
import java.util.*;

public class StringsBuilder{
    public static void main(String args[]){
        StringBuilder sb = new StringBuilder("");
        for(char ch='a'; ch<='z'; ch++){
            sb.append(ch);
        } //abcdefghijklmnopqrstuvwxyz
        System.out.println(sb);
    }
} // Time complexity is o(26)